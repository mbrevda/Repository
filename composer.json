{
    "name": "mbrevda/repository",
    "description": "Abstract repository that support specifications",
    "authors": [
        {
            "name": "Moshe Brevda",
            "email": "mbrevda@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "aura/sqlquery": "2.x-dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "jakub-onderka/php-parallel-lint": "0.*",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Mbrevda\\Repository\\": "src/",
            "Mbrevda\\Repository\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "process-timeout": 30
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c tests/phpunit.xml"

    }
}
